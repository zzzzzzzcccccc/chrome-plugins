{
  "name": "@chrome-plugin/home",
  "version": "1.0.0",
  "description": "Chrome plugin by Home App",
  "license": "MIT",
  "scripts": {
    "start": "next dev",
    "build": "rimraf dist && next build && next export && yarn run build:after",
    "server:start": "next start",
    "build:after": "mv ./out/_next ./out/next && cd ./out && grep -rli '_next' * | xargs -I@ sed -i '' 's/_next/next/g' @ && yarn run move:root",
    "move:root": "mv ./out ./dist && ../../scripts/move.sh ./dist home"
  },
  "dependencies": {
    "@chrome-plugin/common": "^1.0.3",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@monaco-editor/react": "^4.4.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.13",
    "@mui/styled-engine-sc": "^5.11.11",
    "axios": "^1.3.4",
    "i18next": "^22.4.12",
    "i18next-http-backend": "^2.2.0",
    "monaco-editor": "^0.36.1",
    "next": "13.2.4",
    "next-i18next": "^13.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-router-dom": "^6.9.0",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "monaco-editor-webpack-plugin": "^7.0.1"
  }
}
