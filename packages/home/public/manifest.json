{
  "manifest_version": 3,
  "name": "Tabs manager",
  "description": "An application for managing web tabs",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "index.html?render_type=popup",
    "default_icon": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html?render_type=full"
  }
}
