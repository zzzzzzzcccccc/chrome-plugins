{
  "manifest_version": 3,
  "name": "Screenshot OCR",
  "description": "Screenshot and OCR for chrome plugin",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["bundler/inject.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab", "storage", "tabs", "scripting"],
  "optional_host_permissions": ["https://*/*", "http://*/*"]
}
